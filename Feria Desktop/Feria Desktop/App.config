<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="wsFrutosPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/wsFrutos/wsFrutos" binding="basicHttpBinding"
                bindingConfiguration="wsFrutosPortBinding" contract="wsFrutos.wsFrutos"
                name="wsFrutosPort" />
        </client>
    </system.serviceModel>
</configuration>