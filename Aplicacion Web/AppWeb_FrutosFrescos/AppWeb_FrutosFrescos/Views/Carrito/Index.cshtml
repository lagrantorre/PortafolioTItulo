<div class="col-sm-9 padding-right">

    <table id="tabla-carrito">
        <thead>
            <tr>

                <th>Nombre producto</th>
                <th>Cantidad</th>
                <th>Total</th>
            </tr>
        </thead>
        <tbody></tbody>
    </table>


</div>

<script src="https://code.jquery.com/jquery-2.2.4.js"
        integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI="
        crossorigin="anonymous"></script>

<script>

    var carrito = @Html.Raw(Json.Encode(ViewBag.carrito));




    $(document).ready(function(){

        cargaTabla();
     


    });



    

    function cargaTabla(){

        $("#tabla-carrito tbody").html("");

        $.each(carrito, function(k,v){

            var fila = "<tr class='fila-carrito' id-producto='"+ v.idProducto+"'><td>Aca va el nombre del producto</td><td>"+ v.cantidad +"</td><td>Aca va el total</td></tr>";

            $("#tabla-carrito tbody").append(fila);

        });

        
    }



</script>
<script src="https://developers.google.com/maps/documentation/javascript/examples/markerclusterer/markerclusterer.js"></script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAcVi-tXgBUCUmbdt6RI-Rw__U9QCfP220&callback=initMap">
</script>
